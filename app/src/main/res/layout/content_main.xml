<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gg2"
    android:outlineAmbientShadowColor="#000000"
    android:outlineSpotShadowColor="#000000"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constaintLayoutHelp"
        android:layout_width="193dp"
        android:layout_height="183dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="111dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/constraintLayoutEstadisticas"
        app:layout_constraintTop_toBottomOf="@+id/conmstraintLayoutQuestion">

        <Button
            android:id="@+id/buttonHelp"
            android:layout_width="180dp"
            android:layout_height="174dp"
            android:layout_marginStart="52dp"
            android:layout_marginEnd="53dp"
            android:layout_marginBottom="4dp"
            android:background="@drawable/help"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayoutEstadisticas"
        android:layout_width="193dp"
        android:layout_height="183dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="9dp"
        android:layout_marginBottom="111dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/constaintLayoutHelp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayoutStart">

        <Button
            android:id="@+id/buttonViewEstadistics"
            android:layout_width="190dp"
            android:layout_height="174dp"
            android:layout_marginStart="52dp"
            android:layout_marginEnd="53dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/view_stadistics"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/conmstraintLayoutQuestion"
        android:layout_width="202dp"
        android:layout_height="180dp"
        android:layout_marginStart="7dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/constraintLayoutStart"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayoutTitol">

        <Button
            android:id="@+id/buttonAddQuestion"
            android:layout_width="189dp"
            android:layout_height="166dp"
            android:layout_marginStart="57dp"
            android:layout_marginTop="125dp"
            android:layout_marginEnd="57dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/add_question"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayoutTitol"
        android:layout_width="209dp"
        android:layout_height="207dp"
        android:layout_marginTop="36dp"
        android:layout_marginBottom="43dp"
        android:background="@drawable/unnamed"
        android:outlineAmbientShadowColor="@android:color/background_dark"
        android:outlineSpotShadowColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayoutStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayoutStart"
        android:layout_width="193dp"
        android:layout_height="183dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="7dp"
        app:layout_constraintEnd_toStartOf="@+id/conmstraintLayoutQuestion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayoutTitol">

        <Button
            android:id="@+id/buttonStart"
            android:layout_width="188dp"
            android:layout_height="179dp"
            android:layout_marginStart="53dp"
            android:layout_marginEnd="53dp"
            android:background="@drawable/start"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
